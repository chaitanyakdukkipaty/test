require(['jquery','bootstrap','custom'], function ($) {
 setTimeout(function () { 
    const selectElement = document.getElementById('myselection');
    const entryList = document.getElementById('entryList');
    const listItems = entryList?.getElementsByClassName('time-list');
    const selectedValue = $('button.blog-filter.active').attr('data-target');

    let happen = [];
   
    for (let i = 0; i < listItems.length; i++) {
        const item = listItems[i];
        const happening = item.getAttribute('data-item');
        happen.push(happening);
    }
    let uniqueArray = [...new Set(happen)];
    const filtereduniquearr = uniqueArray.filter(item => item !== "" && item !== null);

    if (filtereduniquearr.length > 0) {
        let optionsHTML = `
            <button class="blog-filter active" data-target="all"><span class="icon"><img src="/theme/images/th-online/live-event/blog-filter-all.svg" data-src-template="/theme/images/th-online/live-event/blog-filter-all.svg"></span><span class="text">All</span></button>`;

        uniqueArray.forEach(value => {
            if (value !== '' && value !== null) {
                let replaval = value.includes("-") ? value.replace("-", " ") : value; 
                optionsHTML += `
                    <button class="blog-filter" data-target="${value}"><span class="icon"><img src="/theme/images/th-online/live-event/blog-filter-${value}.svg" data-src-template="/theme/images/th-online/live-event/blog-filter-${value}.svg"></span><span class="text">${replaval}</span></button>`;
            }
        });

        selectElement.innerHTML = optionsHTML;
    }

    $(".live-blog-filters .blog-filter").click(function () { 
        $(".live-blog-filters .blog-filter").removeClass("active");
        $(this).addClass("active");
        
        if ($(this).attr("data-target") === "all") {
            $(".article-live-blog .blog-item").show();
        } else {
            $(".article-live-blog .blog-item").hide();
            $(".article-live-blog .blog-item[data-item='" + $(this).attr("data-target") + "']").show();
        }
    });
}, 1000);
});
